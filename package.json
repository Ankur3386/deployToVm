{
  "name": "deploy",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:generate":"cd packages/db && npx prisma generate && cd ../..",
    "start:http":"cd apps/http && node dist/index.js",
    "start:ws":"cd apps/ws && node dist/index.js",
    "start:frontend":"cd apps/web && pnpm run start"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
